{
  "name": "amo-upload",
  "version": "1.0.0",
  "description": "Upload add-on to AMO",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "engines": {
    "node": ">=20"
  },
  "bin": "dist/bin.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "types/index.d.ts",
  "scripts": {
    "ci": "pnpm run lint",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "dev": "vite dev",
    "clean": "del-cli dist types",
    "build:types": "tsc",
    "build:js": "vite build",
    "build": "pnpm run ci && pnpm run clean && pnpm run /^build:/",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky || true"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "console-table-printer": "^2.14.6",
    "debug": "^4.4.1",
    "es-toolkit": "^1.39.9",
    "jsonwebtoken": "^9.0.2",
    "read-package-up": "^11.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/debug": "^4.1.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  },
  "repository": "git@github.com:violentmonkey/amo-upload.git"
}

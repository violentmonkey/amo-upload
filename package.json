{
  "name": "amo-upload",
  "version": "0.6.1",
  "description": "Upload add-on to AMO",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "engines": {
    "node": ">=20"
  },
  "bin": "dist/bin.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "types/index.d.ts",
  "scripts": {
    "ci": "run-s lint",
    "lint": "eslint --ext .ts,tsx . && prettier --ignore-path .eslintignore -c .",
    "lint:fix": "eslint --ext .ts,tsx . --fix && prettier --ignore-path .eslintignore -c . -w",
    "dev": "rollup -wc",
    "clean": "del-cli dist types",
    "build:types": "tsc",
    "build:js": "rollup -c",
    "build": "run-s ci clean build:*",
    "prepublishOnly": "run-s build",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.15",
    "commander": "^11.0.0",
    "console-table-printer": "^2.11.2",
    "debug": "^4.3.4",
    "es-toolkit": "^1.25.2",
    "jsonwebtoken": "^9.0.2",
    "read-package-up": "^11.0.0"
  },
  "devDependencies": {
    "@gera2ld/plaid": "~2.7.0",
    "@gera2ld/plaid-rollup": "~2.7.0",
    "@types/debug": "^4.1.8",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.6.0",
    "del-cli": "^5.1.0",
    "husky": "^8.0.3"
  },
  "repository": "git@github.com:violentmonkey/amo-upload.git"
}
